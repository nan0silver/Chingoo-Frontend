{
  "buildCommand": "pnpm build:client",
  "outputDirectory": "dist/spa",
  "installCommand": "pnpm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.chingoohaja.app/api/:path*"
    },
    {
      "source": "/ws/:path*",
      "destination": "https://api.chingoohaja.app/ws/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
